		"@capacitor/cli": "^3.3.2",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"@vitejs/plugin-vue": "^1.9.3",
		"@vue/compiler-sfc": "^3.2.22",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.1.1",
		"typescript": "^4.4.3",
		"vite": "^2.6.4",
		"vue-tsc": "^0.3.0"
